{
  "name": "@softwareventures/tslint-rules",
  "version": "0.0.0-development",
  "description": "Standard tslint rules for Software Ventures Limited.",
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/tslint-rules",
  "bugs": "https://github.com/softwareventures/tslint-rules/issues",
  "license": "ISC",
  "repository": "github:softwareventures/tslint-rules",
  "scripts": {
    "prepare": "tsc",
    "test": "tslint --project .",
    "semantic-release": "semantic-release"
  },
  "sideEffects": false,
  "devDependencies": {
    "@softwareventures/prettier-config": "1.0.2",
    "@softwareventures/semantic-release-config": "1.1.1",
    "@softwareventures/tsconfig": "2.3.3",
    "cz-conventional-changelog": "3.3.0",
    "husky": "4.3.8",
    "precise-commits": "1.0.2",
    "prettier": "2.3.0",
    "semantic-release": "17.4.3",
    "tslint": "6.1.3",
    "typescript": "4.3.2"
  },
  "dependencies": {
    "tslib": "^1.9.3 || ^2.0.0 || ^2.1.0",
    "tslint-eslint-rules": "^5.4.0"
  },
  "peerDependencies": {
    "tslint": "^5.12.0 || ^6.0.0"
  },
  "prettier": "@softwareventures/prettier-config",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits"
    }
  },
  "release": {
    "extends": "@softwareventures/semantic-release-config"
  }
}
